{
    "name": "Only One Tab",
    "version": "0.0.0.1",
    "description": "Closes any previously opened tabs with the same URL as current page",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs"
    ],
    "icons": {
        "16": "images/oot16.png",
        "32": "images/oot32.png",
        "48": "images/oot48.png",
        "128": "images/oot128.png"
    },
    "manifest_version": 2
}